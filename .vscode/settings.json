{
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true
  },
  "files.associations": {
    "angular.json": "jsonc",
    "tslint.json": "jsonc"
  },
  "editor.detectIndentation": true,
  "editor.tabSize": 2,
  "html.format.wrapAttributes": "force-aligned",
  "editor.formatOnSave": true,
  "breadcrumbs.enabled": true
}